{
  "expo": {
    "name": "Venera",
    "slug": "evkaliptus-venera",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/media/venus.png",
    "splash": {
      "image": "./assets/media/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#36393E"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.evkaliptus.venera",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.evkaliptus.venera",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/media/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/media/favicon.png"
    },
    "packagerOpts": {
      "config": "metro.config.js"
    }
  }
}
